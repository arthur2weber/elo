  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "axios": "^1.6.0",
    "better-sqlite3": "^11.0.0",
    "body-parser": "^1.20.2",
    "bonjour-service": "^1.3.0",
    "canvas": "^3.2.1",
    "chokidar": "^5.0.0",
    "commander": "^11.1.0",
    "express": "^4.19.2",
    "face-api.js": "^0.22.2",
